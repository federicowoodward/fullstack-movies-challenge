{
  "name": "@fs/api",
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:migration:create": "typeorm-ts-node-commonjs migration:create src/migrations/ManualMigration",
    "db:migration:generate": "typeorm-ts-node-commonjs -d src/config/data-source.ts migration:generate src/migrations/InitSchema",
    "db:migration:run": "typeorm-ts-node-commonjs -d src/config/data-source.ts migration:run",
    "db:migration:revert": "typeorm-ts-node-commonjs -d src/config/data-source.ts migration:revert",
    "db:show": "typeorm-ts-node-commonjs -d src/config/data-source.ts schema:log"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/swagger-ui-express": "^4.1.8",
    "i": "^0.3.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
